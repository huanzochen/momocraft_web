			<!-- Main -->
				<div id="main-wrapper">
					<div class="container">

						<!-- Content -->
							<article class="box login">
                                <section class="sectionlogin">
									<form id="savePost" method="post" action="/register/submit">
										<div class="">
											<span class="login">帳號:  <dfn style="color:red"><%=errorcode %></dfn></span>
											<input type="text" name="account">
										</div>
										<br>
										<div class="">
											<span class="login">密碼:</span>
											<input type="password" name="password" minlength=5 maxlength=16>
										</div>
										<br>
										<div class="">
											<span class="login">再次確認密碼:</span>
											<input type="password" name="passwordCheck" minlength=5 maxlength=<%= fieldlength[(_.map(fieldlength, "COLUMN_NAME").indexOf("password"))].CHARACTER_MAXIMUM_LENGTH %>>
										</div>
										<br>
										<div class="">
											<span class="login">信箱:</span>
											<input type="email" name="email">
										</div>
									</form>
									<div class="submit">
										<button onclick="savePost()">註冊</button>
									</div>
								</section>
							</article>

					</div>
